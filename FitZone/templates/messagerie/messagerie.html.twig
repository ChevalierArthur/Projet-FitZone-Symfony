{% extends 'base.html.twig' %}

{% block title %}HMessagerie!{% endblock %}

{% block body %}
<div class="titre-Accueil">
<h1>Message</h1>
</div>
{% for contac in contact %}
<section class="message">
    <p class="pseudo">{{contac.idUtilisateurcontact.identifiant}}</p>
    <p class="numero">{{contac.idUtilisateurcontact.numTel}}</p><br>
    <p class="email">{{contac.idUtilisateurcontact.email}}</p>
    <p>{{contac.objetContact}}</p>
    <p>{{contac.messageContact}}</p>
    
</section>
{% endfor %}
{% endblock %}
