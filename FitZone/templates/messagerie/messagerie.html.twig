{% extends 'base.html.twig' %}

{% block title %}Hello MessagerieController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>
<div class="titre-Accueil">
<h1>Message</h1>
</div>
{% for contac in contact %}
<section class="message">
    <p>{{contac.idUtilisateurcontact.identifiant}}</p>
    <p class="numero">{{contac.idUtilisateurcontact.numTel}}</p>
    <p>{{contac.idUtilisateurcontact.email}}</p>
    <p>{{contac.messageContact}}</p>
    <p>{{contac.objetContact}}</p>
</section>
{% endfor %}
{% endblock %}
